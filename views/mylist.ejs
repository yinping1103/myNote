<%- include header %>
<div class="container">
	<div class="row">
	    <table class="table table-striped">
		<caption>我的笔记</caption>
		<thead>
		    <tr>
			<th>Title</th>
			<th>Tag</th>
			<th>Date</th>
		    </tr>
		</thead>
		<tbody>
		    <% notes.forEach(function(note){ %>
		    <tr>
			<td><a href="/detail/<%=note._id %>"><%=notes.title %></a></td>
			<td><%=notes.tag %></td>
			<td><%=notes.createTime %></td>
		    </tr>
		    <% }); %>
		</tbody>
	    </table>	
	</div>
</div>
<%- include footer %>
